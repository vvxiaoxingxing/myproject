<script type="text/javascript">
    var indexVm = new Vue({
        el: '#detailsVm',
        data: {
            LocalArray3Data: {},
            Array3CountNum1Data: {},
            Array3CountNum2Data: {},
            Array3CountNum3Data: {},
            Array3CountNextNum1Data: {},
            Array3CountNextNum2Data: {},
            Array3CountNextNum3Data: {},
            LotterNoCount: 100,//期数
            LocalLotterNo: "",//期号
            LiActive: 0,
            fbred: "fb-red",
            fbgray:"fb-gray"
        },
        methods: {
            ChangeTab: function (index) {
                this.LiActive = index;
            },
            GetArray3AnalysisData: function () {
                var that = this;
                $.ajax({
                    method: "get",
                    url: "/Home/GetArray3AnalysisInfo",
                    data: {
                        "lotterNo": this.LocalLotterNo,
                        "lotterNoCount": this.LotterNoCount
                    },
                    dataType: "json",
                    beforeSend: function () {

                    },
                    success: function (data) {
                        console.log(data);
                        if (data.Success) {
                            that.LocalArray3Data = data.Data.LocalArray3Data;
                            console.log(that.LocalArray3Data);
                            that.Array3CountNum1Data = data.Data.EveryBallCount.No1Count;
                            that.Array3CountNum2Data = data.Data.EveryBallCount.No2Count;
                            that.Array3CountNum3Data = data.Data.EveryBallCount.No3Count;

                            that.Array3CountNextNum1Data = data.Data.EveryBallNextCount.No1Count;
                            that.Array3CountNextNum2Data = data.Data.EveryBallNextCount.No2Count;
                            that.Array3CountNextNum3Data = data.Data.EveryBallNextCount.No3Count;
                        } else {
                            console.log("错误....." + data.Msg);
                        }
                    }
                });
            },

        },
        filters: {
            GetCountCorlor: function (data) {
                if (data > 20) {
                    return " fb-red f20";
                } else if (data > 10) {
                    return " fb-green f20";
                }
                else if (data > 5) {
                    return " fb-blue f20";
                } else {
                    return "f20";
                }
            }

        },
        created: function () {

        },
        mounted: function ()
        {

            this.LocalLotterNo = getQueryString("LotterNo");
            this.GetArray3AnalysisData();
        }
    });


    function getQueryString(name) {
        var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
        var r = window.location.search.substr(1).match(reg);
        if (r != null) return unescape(r[2]);
        return null;
    }
</script>

