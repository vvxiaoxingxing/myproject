        <div class="col-lg-12 col-sm-12">
            <table class="col-lg-12 table-responsive-sm" border="1">
                <thead>
                    <tr>
                        <th rowspan="2">#</th>
                        <th rowspan="2">期号</th>
                        <th rowspan="2">开奖日期</th>
                        <th rowspan="2">开奖号码</th>
                        <th colspan="2">直选</th>
                        <th colspan="2">组选3</th>
                        <th colspan="2">组选6</th>
